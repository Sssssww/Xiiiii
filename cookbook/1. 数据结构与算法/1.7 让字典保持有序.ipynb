   "source": [
    "# 让字典有序可以用OrderedDict\n",
    "# 当对字典进行迭代时，它会对按照元素初始添加的排序进行\n",
    "from collections import OrderedDict\n",
    "d = OrderedDict()\n",
    "d['a'] = 1\n",
    "d['b'] = 2\n",
    "d['v'] = 9\n",
    "d['c'] = 4\n",
    "print(d)\n",
    "for i in d:\n",
    "    print(i,d[i])"
